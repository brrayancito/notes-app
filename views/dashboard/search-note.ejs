<div class="container-fluid container-fluid-custom pb-5 mb-5">
  <div class="row">
    <!-- prettier-ignore -->
    <% if (searchResults != '') { %>
           <% searchResults.forEach((note, index) => { %>
    <a href="/dashboard/note/<%= note._id %>">
      <h4><%= note.title %></h4>
    </a>
    <% }) %> <% } else { %>
    <p>No Note found!</p>
    <% } %>
  </div>
</div>
